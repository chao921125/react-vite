{
  "name": "react-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
		"serve": "npm run dev",
		"dev": "vite --mode development --host",
		"build": "npm run build:prod",
		"build:dev": "tsc --noEmit --skipLibCheck && vite build --mode development",
		"build:prod": "tsc --noEmit --skipLibCheck && vite build --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:prettier": "prettier --write --fix --loglevel warn \"./**/*.{js,ts,json,jsx,tsx,css,less,scss,vue,html,md}\"",
		"lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint:lint-staged": "lint-staged",
		"prepare": "husky install",
    "preview": "vite preview",
		"test:gzip": "http-server dist --cors --gzip -c-1",
		"test:br": "http-server dist --cors --brotli -c-1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@types/color": "^3.0.3",
		"@types/js-cookie": "^3.0.3",
		"@types/lodash": "^4.14.194",
		"@types/nprogress": "^0.2.0",
		"@types/qs": "^6.9.7",
		"@types/react": "^18.2.3",
		"@types/react-color": "^3.0.6",
		"@types/react-dom": "^18.2.3",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"@types/react-transition-group": "^4.4.5",
		"@types/redux-logger": "^3.0.9",
		"@types/redux-promise": "^0.5.29",
		"@types/three": "^0.152.0",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"@vitejs/plugin-react": "^4.0.0",
		"@vitejs/plugin-react-swc": "^3.3.0",
		"amfe-flexible": "^2.2.1",
		"autoprefixer": "^10.4.14",
		"babel-plugin-import": "^1.13.6",
		"default-passive-events": "^2.0.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.1",
		"husky": "^8.0.3",
		"less": "^4.1.3",
		"lint-staged": "^13.2.2",
		"postcss-plugin-px2rem": "^0.8.1",
		"postcss-pxtorem": "6.0.0",
		"prettier": "^2.8.8",
		"react-babylonjs": "^3.1.16",
		"react-dev-utils": "^12.0.1",
		"redux-devtools": "^3.7.0",
		"redux-devtools-extension": "^2.13.9",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"stylelint": "^15.6.1",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^4.0.0",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-config-standard-scss": "^9.0.0",
		"stylelint-scss": "^5.0.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.4",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-imp": "^2.4.0",
		"vite-plugin-mock": "^3.0.0",
		"vite-plugin-style-import": "^2.0.0",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-svgr": "^3.2.0"
  },
	"resolutions": {
		"@rollup/plugin-terser": "^0.4.0",
		"bin-wrapper": "npm:bin-wrapper-china",
		"mysql": "^2.18.1"
	},
	"license": "MIT",
	"author": "react-vite",
	"homepage": "https://github.com/vitejs/awesome-vite#plugins",
	"main": "react-vite",
	"repository": {
		"type": "git",
		"url": "https://github.com/vitejs/awesome-vite#plugins"
	},
	"bugs": {
		"url": "https://github.com/vitejs/awesome-vite#plugins"
	},
	"browserslist": [
		"defaults",
		"> 1%",
		"last 2 versions",
		"not ie 11",
		"not op_mini all"
	],
	"engines": {
		"node": ">=16"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
			"prettier --write--parser json"
		],
		"package.json": [
			"prettier --write"
		],
		"*.vue": [
			"stylelint --fix",
			"eslint --fix",
			"prettier --write"
		],
		"*.{css,scss,less,styl,html}": [
			"stylelint --fix",
			"prettier --write"
		],
		"*.md": [
			"prettier --write"
		],
		"*.js": "eslint --cache --fix",
		"*.css": "stylelint --fix"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	}
}
