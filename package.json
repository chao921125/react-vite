		"react-dev-utils": "^12.0.1",
		"redux-devtools": "^3.7.0",
		"redux-devtools-extension": "^2.13.9",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"stylelint": "^15.6.1",
		"stylelint-config-prettier": "^9.0.5",
